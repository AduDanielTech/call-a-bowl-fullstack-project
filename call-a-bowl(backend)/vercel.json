{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      { "source": "/api/:path*", "destination": "/api/:path*" },
      { "source": "/(.*)", "destination": "/$1" }
    ],
    "routes": [
      { "src": "/api/.*", "dest": "/api/index.js" }
    ]
  }
  